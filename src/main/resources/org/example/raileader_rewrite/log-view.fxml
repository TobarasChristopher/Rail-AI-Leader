   <children>
      <Button fx:id="closeButton" layoutX="14.0" layoutY="336.0" mnemonicParsing="false" onAction="#goToMenu" prefHeight="50.0" prefWidth="90.0" text="Back">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <TableView fx:id="logTable" layoutX="20.0" layoutY="20.0" prefHeight="300.0" prefWidth="690.0">
         <columns>
            <TableColumn fx:id="logColumn" prefWidth="689.0" text="Log Entry" />
         </columns>
      </TableView>
      <Button fx:id="exportButton" layoutX="114.0" layoutY="336.0" mnemonicParsing="false" onAction="#exportToCSV" prefHeight="25.0" prefWidth="90.0" text="Export to CSV" />
   </children>
</AnchorPane>
